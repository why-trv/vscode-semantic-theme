{
  "name": "Semantic Theme",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#c4c4c4",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "semanticTokenColors": {
    "namespace": "#ccb87a"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5c5c5c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#cecece"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
        "foreground": "#C681E1"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9c81e1"
      }
    },
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#c774cb",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#4cadd3"
      }
    },
    {
      "name": "Template Argument Name",
      "scope": [
        "entity.name.type.template.cpp"
      ],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#97c8ef"
      }
    },
    {
      "name": "Cast",
      "scope": [
        "keyword.operator.cast"
      ],
      "settings": {
        "foreground": "#4f799a"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Primitive Type",
      "scope": [
        "storage.type.primitive",
        "storage.type.built-in"
      ],
      "settings": {
        "foreground": "#C681E1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#e0b569"
      }
    },
    {
      "name": "Class, Struct, Type Declaration",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.struct",
        "entity.name.type.typedef"
      ],
      "settings": {
        "foreground": "#e0b569",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class Template Instantiation?",
      "scope": [
        "entity.name.type.class.templated"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Namespace Prefix",
      "scope": [
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#ccb87a"
      }
    },
    {
      "name": "Lambda Return Type",
      "scope": [
        "storage.type.return-type.lambda.cpp"
      ],
      "settings": {
        "foreground": "#c2a880"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "entity.name.function.member",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "name": "Member Function",
      "scope": [
        "entity.name.function.definition",
        "keyword.other.operator.overload.cpp"
      ],
      "settings": {
        "foreground": "#61AFEF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Static Function",
      "scope": [
        "entity.name.function.member.static"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Special Function",
      "scope": [
        "entity.name.function.definition.special"
      ],
      "settings": {
        "foreground": "#8AA4F1",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Macro",
      "scope": [
        "entity.name.function.preprocessor",
        "keyword.control.directive"
      ],
      "settings": {
        "foreground": "#3da779"
      }
    },
    {
      "name": "Property/Member Variables",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.body.class variable.other.declare"
      ],
      "settings": {
        "foreground": "#f0787f"
      }
    },
    {
      "name": "Local Variable Declaration",
      "scope": [
        "meta.body.function.definition variable.other.declare"
      ],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#f0787f"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#f9a167"
      }
    },
    {
      "name": "Argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#cacaca",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword (Other)",
      "scope": [
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#C681E1"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#c1e88d"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "TypeScript Primitive",
      "scope": [
        "support.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#3da779"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
